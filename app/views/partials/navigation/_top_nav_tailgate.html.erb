<li class="fanzone" id="nav_<%= aTailgate.slug -%>">
  <a href="<%= tailgate_path( aTailgate ) %>" title="<%= aTailgate.name %>">
    <table>
      <tr>
        <td class="team_logo"><%= teamLogo(aTailgate.team, :small) -%></td>
        <td class="fanzone_name"><%= aTailgate.name.length < 27 ? aTailgate.name : aTailgate.name[0, 23] + "..." -%></td>
      </tr>
    </table>
  </a>
</li>
